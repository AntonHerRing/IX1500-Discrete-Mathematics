 RowBox[{
  RowBox[{"ascii", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"s", "=", 
      RowBox[{"testm", "[", 
       RowBox[{"[", "n", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"s", "\[NotEqual]", "0"}], ",", "\[IndentingNewLine]", 
